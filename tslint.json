{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "align": false, // [ true, "parameters", "arguments", "statements" ],
    "ban": false,
    "class-name": true,
    "comment-format": [true, "check-space"],
    "curly": true,
    "eofline": false,
    "forin": true,
    "indent": [true, "spaces"],
    "interface-name": [true, "never-prefix"],
    "jsdoc-format": true,
    "prefer-for-of": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false, // changed
    "object-literal-sort-keys": false, // added
    "no-var-requires": false, // added
    "jsx-wrap-multiline": false,
    "label-position": true,
    "max-line-length": [
      false, // changed
      120
    ],
    "no-misused-new": false,
    "member-ordering": [
      true,
      "private-before-public",
      "static-before-instance",
      "variables-before-functions"
    ],
    "no-any": false,
    "no-arg": true,
    "no-bitwise": false,
    "no-consecutive-blank-lines": true,
    "no-console": false,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": false, // changed
    "no-eval": true,
    "no-shadowed-variable": false, // changed
    "jsx-self-close": false, // added
    "no-string-literal": true,
    "no-submodule-imports": false,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "no-unused-expression": true,
    "prefer-conditional-expression": false, // added
    "ordered-imports": false, // added
    "one-variable-per-declaration": false, // added
    "no-use-before-declare": false, // changed
    "one-line": [
      true,
      "check-catch",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "radix": true,
    "semicolon": false, // edit [true, "always"]
    "switch-default": true,
    "trailing-comma": false,
    "triple-equals": [true, "allow-null-check"],
    "typedef": false, // changed [ true, "parameter", "property-declaration"],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": false, // changed [true, "ban-keywords", "check-format", "allow-leading-underscore", "allow-pascal-case"],
    "jsx-alignment": false, // added
    "prefer-const": false, // added
    "no-implicit-dependencies": false, // added
    "no-duplicate-imports": false, // added
    "max-classes-per-file": false, // added
    "jsx-no-bind": false, // added
    "only-arrow-functions": false, // added
    "schedule": false, // added
    "quotemark": false, // changed "quotemark": [true, "single", "jsx-double"],
    "arrow-parens": false, //added
    "object-literal-key-quotes": false, // added
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast"
    ]
  }
}
